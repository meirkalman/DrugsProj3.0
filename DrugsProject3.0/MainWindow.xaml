<Window
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:DrugsProject3._0"
        xmlns:Controls="clr-namespace:DrugsProject3._0.Controls"
    xmlns:materialDesign="http://materialdesigninxaml.net/winfx/xaml/themes" x:Class="DrugsProject3._0.MainWindow"
        mc:Ignorable="d"
        Title="MainWindow" Height="604.038" Width="980">
   
    <Grid  Name="MainWindowGrid" >
        <Grid.RowDefinitions>
            <RowDefinition/>
        </Grid.RowDefinitions>

        <materialDesign:ColorZone
  Mode="PrimaryLight"
  Padding="16">
            <StackPanel
    Orientation="Horizontal">
                <ToggleButton
      Style="{DynamicResource MaterialDesignHamburgerToggleButton}" />
                <TextBlock
      VerticalAlignment="Center"
      Margin="16 0 0 0">
    Material Design In XAML Toolkit
                </TextBlock>
            </StackPanel>
        </materialDesign:ColorZone>
        <Grid Name="InternalGrid" HorizontalAlignment="Left" Height="505" Margin="0,69,0,0" VerticalAlignment="Top" Width="972">
            <Button x:Name="button" Command="{Binding MainCommand}" CommandParameter ="DoctorUC"   Content="doctor" HorizontalAlignment="Left" Margin="124,120,0,0" VerticalAlignment="Top" Width="173"  />
            <Button Content="doctor visit2" HorizontalAlignment="Left" Margin="124,225,0,0" VerticalAlignment="Top" Width="173" />
            <Button Content="home page"   Command="{Binding MainCommand}" CommandParameter ="HomePageUC" HorizontalAlignment="Left" Margin="124,174,0,0" VerticalAlignment="Top" Width="173" />
            <Button Content="admin" HorizontalAlignment="Left" Margin="124,274,0,0" VerticalAlignment="Top" Width="173" />
        </Grid>
        

    </Grid>

  

</Window>
